{
  "name": "node-red-contrib-{{project-name}}",
  "version": "0.0.0",
  "description": "Boilerplate for creating a Node-RED node",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && node ../../common/scripts/copy-html.js",
    "test": "jest --config ./jest.unit.config.js"
  },
  "node-red": {
    "nodes": {
      "{{node-name}}": "lib/{{node-name}}/{{node-name}}.js"
    }
  },
  "keywords": [
    "node-red"
  ],
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "author": "JerryWu9610<zlcatwu@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "~29.1.2",
    "@types/node": "~18.8.5",
    "@types/node-red": "^1.3.4",
    "@typescript-eslint/eslint-plugin": "~5.39.0",
    "@typescript-eslint/parser": "~5.39.0",
    "@vue/eslint-config-prettier": "~7.0.0",
    "eslint": "~8.24.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-prettier": "~4.2.1",
    "jest": "~29.1.2",
    "prettier": "~2.7.1",
    "ts-jest": "~29.0.5",
    "typescript": "~4.8.4"
  }
}
